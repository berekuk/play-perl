</div>
<div class="app-view-container">
<div class="app-view-notifies"></div>
<div><article class="quest-page-unregistered">

<aside class="sidebar quest-page-sidebar">

  <div class="quest-page-reward">
    <div>

    <div class="reward reward-status-open">
      <span class="reward-points"><% quest.points %></span>

        <span class="icon-stack" data-toggle="tooltip" title="" data-original-title="Reward&nbsp;points">
          <i class="icon-circle icon-stack-base"></i>
          <i class="icon-star icon-light"></i>
        </span>
      <!--
 --></div>
    </div>
    <span class="label label-warning"><% quest.status %></span>
  </div>





  <div class="quest-checkins-sv"><div class="quest-checkins hide">
  <header>Checkins:</header>
  <div class="quest-checkin-none">None so far</div> <!-- TODO -->


</div>
</div>

</aside>

<div class="mainarea">
  <div class="main">
    <div class="quest-big"><div class="post-big quest-big quest-big-open">
  <aside>


    <% FOREACH user in quest.team %>
      <div class="post-big-upic-box">
        <a href="/player/<% user %>"><img src="/api/user/<% user %>/pic?s=small" alt="<% user %>" title="<% user %>" class="upic-small"></a>
      </div>
    <% END %>


  </aside>

  <div class="post-big-inner">
    <div>
      <small class="muted">
        <span class="muted">
          <% IF quest.tealm.size %>
            <% FOREACH user in quest.team %>
            <a href="/player/<% user %>" class="user-link"><% user %></a><% ", " UNLESS loop.last %>
            <% END %>
            <% IF quest.status == 'open' %>
              <% quest.team.size == 1 ? 'is' : 'are' %> on a quest: <!-- TODO - "completed", "suggests" -->
            <% ELSIF quest.status == 'closed' %>
              completed a quest:
            <% ELSE %>
              was on a quest:
            <% END %>
          <% ELSE %>
            <a href="/player/<% quest.author %>" class="user-link"><% quest.author %></a> suggests a quest:
          <% END %>

        </span>
      </small>
    </div>

    <header class="post-big-title">
      <span class="quest-big-editable"><% quest.name %></span>
    </header>

    <% IF quest.description %>
    <div class="widetext">
      <div class="quest-big-description-sv">
        <% markdown(quest.description, quest.realm) %>
      </div>
    </div>
    <% END %>

    <span class="muted quest-big-edit control-group">
      <input name="tags" type="text" placeholder="Tags" data-placement="top" data-title="tags must be alphanumerical" data-animation="false" data-trigger="manual">

      <div class="post-edit-controls pull-right">
        <button class="btn cancel">Cancel</button>
        <button class="btn btn-primary save">Save</button>
      </div>
    </span>


    <footer class="object-bottom-line">
      <div class="quest-big-tags">
        <span class="quest-tags">
          <% FOREACH tag IN quest.tags %>
            <a href="/realm/<% quest.realm %>/explore/latest/tag/<% tag %>" class="tag ">
              <% tag %>
            </a>
          <% END %>
        </span>
      </div>

      <time class="timeago" datetime="<% quest.updated %>" title="<% quest.updated %>"><% quest.updated %></time>

      <span class="likes-subview">
    <!--

 --><span class="like-button " data-toggle="tooltip" title="" data-original-title="<% quest.likes.join(', ') %>">
      <i class="icon-thumbs-up-alt"></i>
      <% quest.likes ? quest.likes.size : 0 %>
  </span><!--

 -->

</span>

    </footer>

    <div class="post-realm">
      <a href="/realm/<% quest.realm %>" class="post-realm-link">
        <img src="/i/<% quest.realm %>.png" width="30" height="30">
        Meta
      </a>
    </div>
  </div>

</div>
</div>

<div class="comments"><div class="comments-list" style="">

<% FOREACH comment IN comments %>
<div class="comment-outer">
<article class="comment comment-type-text">
  <aside>
    <a href="/player/<% comment.author %>"><img src="/api/user/<% comment.author %>/pic?s=small" alt="<% comment.author %>" title="<% comment.author %>" class="upic-small"></a>
  </aside>

  <div class="comment-inner">

    <header class="object-top-line">
      <a href="/player/<% comment.author %>" class="user-link"><% comment.author %></a>
    </header>

    <div class=" comment-content">
      <% markdown(comment.body, quest.realm) %>
    </div>

    <footer class="object-bottom-line">
      <time class="timeago" datetime="<% comment.updated %>" title="<% comment.updated %>"><% comment.updated %></time>

    <span class="likes">
      <span class="like-button  like-button-hide" data-toggle="tooltip" title="" data-original-title="">
      <i class="icon-thumbs-up-alt"></i>
      <% comment.likes ? comment.likes.size : 0 %>
  </span><!--

 -->

</span>
    </footer>
  </div>
</article></div>
<% END %>

</div>

<div class="comment-add-sv"><div class="comment-add">

    <div class="comment-add-signin">
      <header>Sign in to comment:</header>
      <span class="signin"><div class="signin ">
  <button class="btn  btn-primary login-with-twitter">Sign Up with Twitter</button>
  <button class="btn  btn-primary login-with-persona">Sign Up with Email</button>
</div>
</span>
    </div>

</div>
</div>
</div>
  </div>
</div>

</article>
</div></div>
<div id="push"></div>
</div>
