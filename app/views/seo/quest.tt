  <div class="current-user-box"><!-- current user widget from right-top corner -->
<ul class="top-nav">
  <li class="dropdown">

      <!-- TODO: refactor to use views/user/signin.js -->
      <a href="#" class="dropdown-toggle" data-toggle="dropdown">
