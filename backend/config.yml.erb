hostport: <%= ENV['QH_HOST'] %>:<%= ENV['QH_PORT'] %>
service_name: Questhub.io

aws:
    access_key_id: <%= ENV['AWS_ACCESS_KEY_ID'] || 'NONE' %>
    secret_access_key: <%= ENV['AWS_SECRET_ACCESS_KEY'] || 'NONE' %>

storage_type: <%= ENV['S3_BUCKET'] ? 's3' : 'local' %>
s3_bucket: <%= ENV['S3_BUCKET'] || 'NONE' %>

unsubscribe_salt: <%= ENV['UNSUBSCRIBE_SALT'] || 1234567890 %>
mixpanel_token: <%= ENV['MIXPANEL_TOKEN'] || 1234567890 %>
data_dir: "/data"
